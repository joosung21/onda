include includes/header.pug
body(name='notice')
  include includes/topmenu.pug
  .left
    h1
      img.logo(src='/images/logo.svg')
      |  NOTICE
    ul.post-nav
      each post in data.objects
        li= post.title

  .content
    each post in data.objects
      .post
        h2.post-title= post.title
        .date #{moment(post.created_at).format('YYYY-MM-DD h:mma')}
        .post-content!= post.content